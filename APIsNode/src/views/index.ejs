    <div class="w3-container">
      <h2 style="padding-left: 20px ;position: relative;top: 30px">Giỏ hàng của tôi</h2>
      <p></p>
    
      <table class="w3-table w3-bordered" style="position:relative; top: 50px; left: 20px; right: inherit;;">
        <thread>
          <tr class="w3-table-title">
            <th>Mã đơn</th>
            <th>Tên sản phẩm</th>
            <th>Số lượng</th>
            <th>Thành tiền</th>
            <th>Tên khách hàng</th>
            <th>Số điện thoại</th>
            <th>Địa chỉ</th>
            <th>              </th>
          </tr>
        </thread>
        <tbody>
          <% for(let i=0; i < dataUser.length; i++) { %>
            <tr>
              <td><%= dataUser[i].id %></td>
              <td><%= dataUser[i].fullname %></td>
              <td style=" text-align: center ;"><%= dataUser[i].amount %></td>
              <td><%= new Intl.NumberFormat().format((dataUser[i].amount)*3000000) %> VND</td>
              <td><%= dataUser[i].customername %></td>
              <td><%= dataUser[i].phoneNumber %></td>
              <td style="display:flexbox; width: 250px ;"><%= dataUser[i].address %></td>
              <td style="display:flex ;">
                <a class="buttona" href="/detail/orders/<%= dataUser[i].id %>" target="_blank">Detail</a>
                <form action="/edit-order/<%= dataUser[i].id %>" method="GET">
                <button class="buttona" type="submit">Edit</button>
              </form>
                <form action="/delete-order" method="POST">
                  <input type="text" hidden value="<%= dataUser[i].id %>" name="orderID">
                <button class="buttona" type="submit">Delete</button>
              </form>
              </td>

            </tr>
         <% } %>
        </tbody>
      </table>
    </div>
    
